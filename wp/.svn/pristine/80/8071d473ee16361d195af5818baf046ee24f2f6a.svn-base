/* global */
html,body {font-size:100%;margin:0px;padding:0px;width:100%;height:100%;background:#FFFFFF;color:#333333;font:12px arial,"Microsoft Yahei","Hiragino Sans GB",sans-serif;}
body {overflow-y:scroll;}
ul, ul li {list-style:none;}
h1, h2, h3, h4, h5, h6, ul, li {font-weight:normal;padding:0;margin:0;}
a {text-decoration:none;outline:none;color:#66a;}
input, select{outline:none;}
img {border:none;}

/* float */
.float_l{float:left;}
.float_r{float:right;}
.hidden{display: none;}

/*颜色值*/
.color_red{color:red}

.list_inline li{display:inline-block;}
input,textarea,select,button,.button{border:1px solid #666;background:#fff;border-radius:5px;}
input,select{width:180px;height:18px;}
button:hover,.button:hover{color:#FF6500;cursor: pointer;}

.border_1px{border:1px solid #666;}
.bgcolor{background-color:#eee;}
.header{height:30px;}
.submenu{position:absolute;display:none;background-color:#fff;}
.body{
	padding:30px 0px 30px;
	width:80%;
	margin:0px auto 0px auto;
}
#project_list li{display:inline-block;width:100px;padding-bottom:10px;}

.tab{margin-left:10%;}
.tab li{display:inline-block;}
.tab li button{border-radius:4px 4px 0px 0px;background:#eee;}
.tab li .current{border-bottom:1px solid #fff;background:#fff;}
.tab_content{
	min-height:500px;
	border:1px solid #333;
	border-radius:5px;
	margin-top:-1px;
	
}
.view_panel{height:470px;}
.view_table{width:100%;border-spacing:0px;padding:3px 10px 3px 10px;}
.view_table tr.title{height:30px;}
.view_table tr{height:60px;}
.view_table tr td,.view_table tr th{border-bottom:1px solid #666;}
.view_table tr:hover td{background:#eee;}

.paging{text-align:right;padding:3px 20px;}

.dialog{border:1px solid #666;min-width:400px;min-height:300px;border-radius:5px;position:absolute;z-index:9999;left:30%;top:20%;background:#fff;}
.dialog .dialog-title{text-align:right;background:#eee;}
.dialog .dialog-title button{background:#eee;}
.dialog .dialog-content{padding:10px;}

.select_bar{padding:20px;}